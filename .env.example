# Telegram
TELEGRAM_BOT_TOKEN="paste"
TELEGRAM_CHAT_ID="paste"

# Market provider: kis | fdr_daily | pykrx_daily
DATA_PROVIDER="kis"

# KIS OpenAPI
KIS_APP_KEY=""
KIS_APP_SECRET=""
KIS_ACCOUNT_NO=""
KIS_IS_PAPER="false"

# Universe/filters
UNIVERSE="KOSPI,KOSDAQ"
TOP_N="5"

# Scheduling window (KST)
RUN_HOURLY_START="08:00"
RUN_HOURLY_END="17:00"

# DB
SQLITE_PATH="data/money.db"

# Guardrails
MIN_VALUE_KRW="1000000000"
MAX_ABS_RETURN_1H="0.20"

# Optional analyst model (local Ollama)
ANALYST_ENABLE="false"
ANALYST_BACKEND="ollama"
ANALYST_MODEL="gemma3:12b"

# Virtual paper trading
PAPER_ENABLE="true"
PAPER_INITIAL_CASH="1000000"
PAPER_MAX_TRADES_PER_DAY="10"
PAPER_MAX_POSITIONS="3"
PAPER_FEE_BPS="1.5"
PAPER_SLIPPAGE_BPS="3.0"

# US index context
SP500_ENABLE="true"

# Event/news risk context
EVENT_RISK_ENABLE="true"
EVENT_FEED_URLS="https://feeds.finance.yahoo.com/rss/2.0/headline?s=%5EGSPC&region=US&lang=en-US,https://feeds.finance.yahoo.com/rss/2.0/headline?s=%5EKS11&region=US&lang=en-US"
# Format: YYYY-MM-DD:LABEL;YYYY-MM-DD:LABEL
HIGH_IMPACT_DATES=""

# Strategy lab (nightly experiments)
STRATEGY_LAB_ENABLE="true"
