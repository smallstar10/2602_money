[Unit]
Description=2602_money intraday training status report
After=network-online.target

[Service]
Type=oneshot
WorkingDirectory=%h/2602_money
Environment=PYTHONUNBUFFERED=1
ExecStart=%h/2602_money/.venv/bin/python %h/2602_money/src/jobs/run_intraday_training_status.py
