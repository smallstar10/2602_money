[Unit]
Description=2602_money evening integrated report
After=network-online.target

[Service]
Type=oneshot
WorkingDirectory=%h/2602_money
Environment=PYTHONUNBUFFERED=1
ExecStart=%h/2602_money/.venv/bin/python %h/2602_money/src/jobs/run_evening_report.py
StandardOutput=journal
StandardError=journal
